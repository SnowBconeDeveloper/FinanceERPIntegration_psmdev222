<?xml version="1.0" encoding="UTF-8"?>
<database>
	<!-- credentials -->
 	<element name="sn_fcms_intg_netsuite_credentials" type="collection" label="NetSuite Credentials" extends="discovery_credentials">
 		<element name="type" default_value="netsuite">
			<choice>
				<element label="NetSuite" value="netsuite"/>
			</choice>
		</element>
 		<element name="nlauth_account" type="string" max_length="100" mandatory="true" label="NLAuth Account" hint="NetSuite Authorization Account"/>
		<element name="nlauth_email" type="string" max_length="150" mandatory="true" label="NLAuth Email" hint="NetSuite Email Id"/>
		<element name="nlauth_role" type="string" max_length="100" mandatory="true" label="NLAuth Role" hint="NetSuite Authorization Role"/>
		<element name="script" type="string" max_length="100" mandatory="true" label="Script" hint="NetSuite Query Parameters script value"/>
		<element name="deploy" type="string" max_length="100" mandatory="true" label="Deploy" hint="NetSuite Query Parameters deploy value"/>
	</element>
</database>